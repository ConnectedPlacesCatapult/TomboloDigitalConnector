{
  "dataset": {
    "subjects": [
      {
        // The output subjects are all LSOAs whose name starts with 'Leeds'
        "subjectType": "lsoa",
        "provider": "uk.gov.ons",
        "matchRule": {
          "attribute": "name",
          "filter": "startsWith",
          "value": "Leeds"
        }
      }
    ],
    "datasources": [
      {
        // Importer for LSOA geographies
        "importerClass": "uk.org.tombolo.importer.ons.OaImporter",
        "datasourceId": "lsoa"
      }
    ],
