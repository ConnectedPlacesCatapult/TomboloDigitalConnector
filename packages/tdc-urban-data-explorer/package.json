{
  "name": "tombolo-view",
  "version": "0.0.1",
  "author": "Robin Summerhill",
  "description": "Tombolo Viewer App",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc",
    "start": "pm2-docker --web 3001 process.yml",
    "dev": "DEBUG=tombolo:* nodemon --inspect=3001 --ext ts --watch src --exec './node_modules/.bin/ts-node' src/www.ts",
    "lint": "tsconfig-lint",
    "test": "NODE_ENV=test nodemon --ext ts --watch src --watch test --exec 'jasmine-ts' JASMINE_CONFIG_PATH=./jasmine.json",
    "gen-doc": "typedoc --out doc --mode modules --readme README.md",
    "view-doc": "http-server doc -p 8001 -o",
    "version": "cd src && node-git-version & cd .."
  },
  "main": "build/www.js",
  "dependencies": {
    "@mapbox/mapbox-gl-style-spec": "^10.1.0",
    "@mapbox/sphericalmercator": "^1.0.5",
    "@mapbox/tilelive": "^5.12.6",
    "@types/handlebars": "^4.0.36",
    "@types/lodash": "^4.14.104",
    "@types/multer": "^1.3.6",
    "@types/passport": "^0.4.3",
    "base58": "^1.0.1",
    "body-parser": "~1.16.0",
    "compression": "^1.7.1",
    "config": "^1.26.2",
    "connect-session-sequelize": "^5.2.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "d3-scale": "^2.0.0",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "express-handlebars": "^3.0.0",
    "express-jwt": "^5.3.0",
    "express-jwt-permissions": "^1.0.0",
    "express-query-boolean": "^1.0.0",
    "express-session": "^1.15.6",
    "express-validation": "^1.0.2",
    "express-validator": "^4.2.0",
    "handlebars": "^4.0.10",
    "joi": "^11.0.3",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^8.0.1",
    "lru-cache": "^4.1.1",
    "mapnik": "^3.6.2",
    "mbtiles": "^0.9.0",
    "moment": "^2.22.1",
    "morgan": "~1.7.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "6",
    "reflect-metadata": "^0.1.10",
    "sequelize": "4.8.4",
    "sequelize-typescript": "0.5.0",
    "serve-favicon": "~2.3.2",
    "tilelive-modules": "^0.3.4",
    "toml": "^2.3.3",
    "typedi": "^0.5.2",
    "url-polyfill": "^1.0.8",
    "uuid": "^3.2.1",
    "web-animations-js": "^2.3.1",
    "winston": "^2.3.1",
    "wkx": "^0.4.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/express": "^4.0.35",
    "@types/jasmine": "^2.2.33",
    "@types/jsonwebtoken": "^7.2.3",
    "@types/lru-cache": "^4.1.0",
    "@types/node": "^7.0.8",
    "@types/nodemailer": "^3.1.3",
    "@types/pg": "^7.1.2",
    "@types/sequelize": "^4.0.73",
    "@types/winston": "^2.3.6",
    "http-server": "^0.10.0",
    "jasmine": "2.4.1",
    "jasmine-ts": "^0.2.1",
    "node-git-version": "^0.1.2",
    "nodemon": "^1.11.0",
    "rewire": "^2.5.2",
    "sequelize-cli": "^3.0.0-3",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "tsconfig-lint": "^0.11.0",
    "tslint": "^3.15.1",
    "typedoc": "^0.11.1",
    "typedoc-plugin-external-module-name": "^1.0.9",
    "typescript": "^2.5.2"
  }
}
